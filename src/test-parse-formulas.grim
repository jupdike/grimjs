        1/z + Sum(1/(z-m-n*tau)+1/(m+n*tau)+z/(m+n*tau)**2, _mn_cond))),
    Variables(z, tau),
    Assumptions(And(Element(z, CC), Element(tau, HH), NotElement(z, Lattice(1, tau)))))

make_entry(ID("7c4457"),
